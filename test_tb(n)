from random import seed


from random import randint



def test_tb(n):


    seed(1)

    total_score = 0

    for i in range(n):

        one = randint(0, 10)

        two = randint(0, 10)

        answer =int(input((one, "*", two, "=")))

        if answer == one*two:
            total_score = total_score + (100/n)
